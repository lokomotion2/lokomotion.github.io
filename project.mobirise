    "Informaci√≥n para vendedores.html": {
      "settings": {
        "meta_descr": "Informaci√≥n para vendedores",
        "title": "Informaci√≥n para vendedores",
        "order": 2
      },
      "components": [
        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
